{
  "version": "1",
  "exif-retain": ["DateTime", "DateTimeOriginal"],
  "resize": [
    {
      "folder": "resized/sd",
      "width": 640,
      "height": 720,
      "compression": 50
    },
    {
      "folder": "resized/hd",
      "width": 1280,
      "height": 720,
      "compression": 50
    },
    {
      "folder": "resized/fhd",
      "width": 1920,
      "height": 1080,
      "compression": 50
    }
  ],
  "video": {
    "width": 1920,
    "height": 1080,
    "source": "resized/fhd",
    "fps": 30
  }
}
